    <validationRules>
        <fullName>Reject_Reason_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(Final_Decision__c, &quot;Rejected&quot;), ISBLANK(Disbursal_Reject_Reason__c))</errorConditionFormula>
        <errorMessage>Reject Reason cannot be blank</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Approval_Date</fullName>
        <active>false</active>
        <errorConditionFormula>AND(ISPICKVAL(Final_Decision__c, &quot;Approved&quot;), ISNULL(Disbursal_Approval_Date__c))</errorConditionFormula>
        <errorMessage>Approval Date should not be null</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Final_Decision_Date</fullName>
        <active>false</active>
        <errorConditionFormula>AND(ISNULL(Final_Decision_Date__c),
OR(ISPICKVAL(Final_Decision__c, &quot;Approved&quot;),
ISPICKVAL(Final_Decision__c, &quot;Rejected&quot;)
))</errorConditionFormula>
        <errorMessage>Final Decision Date should not be null</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Repayment_Begins_On</fullName>
        <active>false</active>
        <errorConditionFormula>AND(ISPICKVAL(Final_Decision__c, &quot;Approved&quot;), ISNULL(First_Repayment_Date__c))</errorConditionFormula>
        <errorMessage>Repayment Begins On should not be null</errorMessage>
    </validationRules>
    <webLinks>
        <fullName>Generate_Advanced_Notification</fullName>
        <availability>online</availability>
        <description>Advanced Notifications of Operations are forwarded to users.</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Generate Advanced Notification</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/apex/dsfs__DocuSign_JavaScript&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/22.0/connection.js&quot;)} 

var newRecords= []; 
var c = new sforce.SObject(&quot;Operation__c&quot;); 
c.id =&quot;{!Operation__c.Id}&quot;;
c.Generate_Advanced_Notification__c = true;
newRecords.push(c); 
result1 = sforce.connection.update(newRecords);

var result = sforce.connection.query(&quot;Select Name, Account__c, Account__r.PersonContactId, Opportunity__c, Person_Account_Email_ID__c, Person_Account_Role__c, Email__c, Account_Role__c From Guarantors__c where Opportunity__c = &apos;{!Operation__c.OpportunityId__c}&apos;&quot;); 

var redirectURL; 

records = result.getArray(&quot;records&quot;); 

var DocuSignRId; 
var DocuSignRType; 
var DocuSignRRole; 
var DocuSignRRoutingOrder;
var Signer;

for(var i=1; i&lt;=records.length; i++) 
{ 
DocuSignRId = &apos;&apos;; 
DocuSignRType = &apos;&apos;; 
DocuSignRRole = &apos;&apos;; 
DocuSignRRoutingOrder = &apos;&apos;; 
Signer = &apos;&apos;;

var record = records[i-1]; 

if(i == 1) 
{ 
DocuSignRId = &apos;DocuSignR&apos; + [i] + &apos;Id&apos;; 
DocuSignRType = &apos;DocuSignR&apos; + [i] + &apos;Type&apos;; 
DocuSignRRole = &apos;DocuSignR&apos; + [i] + &apos;Role&apos;; 
DocuSignRRoutingOrder = &apos;DocuSignR&apos; + [i] + &apos;RoutingOrder&apos;; 
Signer = &apos;Signer &apos; + [i];

redirectURL = &apos;https://composer.congamerge.com?sessionId={!API.Session_ID}&amp;serverUrl={!API.Partner_Server_URL_290}&amp;id={!Operation__c.Id}&amp;QueryId=a0D26000000DrW0&amp;TemplateId=a0E260000004Ige&amp;LG4=1&amp;QueryId=a0D26000000DuWU&amp;LG4=1&amp;LGAttachOption=Attachments &amp;LG3=1&amp;sc=1&amp;SC1=Attachments&amp;DefaultPDF=1&amp;AttachmentParentID{!Operation__c.Id}&amp;ReportId=[operation]00O26000000F9Cg?pv0={!Operation__c.Id}&amp;&apos; + DocuSignRId + &apos;=&apos; + record.Account__r.PersonContactId + &apos;&amp;&apos; + DocuSignRType + &apos;=Signer&amp;&apos; + DocuSignRRole + &apos;=&apos; + Signer + &apos;&amp;&apos; + DocuSignRRoutingOrder + &apos;=&apos; + i; 
} 
else 
{ 
DocuSignRId = &apos;DocuSignR&apos; + [i] + &apos;Id&apos;; 
DocuSignRType = &apos;DocuSignR&apos; + [i] + &apos;Type&apos;; 
DocuSignRRole = &apos;DocuSignR&apos; + [i] + &apos;Role&apos;; 
DocuSignRRoutingOrder = &apos;DocuSignR&apos; + [i] + &apos;RoutingOrder&apos;; 
Signer = &apos;Signer &apos; + [i];

redirectURL = redirectURL + &apos;&amp;&apos; + DocuSignRId + &apos;=&apos; + record.Account__r.PersonContactId + &apos;&amp;&apos; + DocuSignRType + &apos;=Signer&amp;&apos; + DocuSignRRole + &apos;=&apos; + Signer + &apos;&amp;&apos; + DocuSignRRoutingOrder + &apos;=&apos; + i; 
} 
} 
redirectURL = redirectURL + &apos;&amp;DocuSignVisible=1&amp;DS7=7&apos;; 

location.href = redirectURL;</url>
