    <fields>
        <fullName>Application_Documentation__c</fullName>
        <externalId>false</externalId>
        <formula>HYPERLINK(
 IF( 
CONTAINS($Api.Enterprise_Server_URL_260, &quot;cs81&quot;), 
&quot;https://advancedpay.sharepoint.com/sites/ampdev/&quot;, 
IF( 
CONTAINS($Api.Enterprise_Server_URL_260, &quot;cs80&quot;), 
&quot;https://advancedpay.sharepoint.com/sites/QA/&quot;, 
&quot;https://sharepoint.amp-creditech.com/sites/clientdocs/&quot; 
) 
)+Country__c+&quot;/Forms/AllItems.aspx?RootFolder=%2Fsites%2Fampdev%2F&quot;+Country__c+&quot;%2F&quot; + 
 SUBSTITUTE(
    SUBSTITUTE(
     SUBSTITUTE(
      SUBSTITUTE(
       SUBSTITUTE(
        SUBSTITUTE(
         SUBSTITUTE(
          SUBSTITUTE(
           SUBSTITUTE(
            SUBSTITUTE(
             SUBSTITUTE(
              SUBSTITUTE(
               SUBSTITUTE(
                SUBSTITUTE(
                 SUBSTITUTE(Opportunity__r.Account.Name, &quot;&amp;&quot;, &quot;-&quot;)
                , &quot;/&quot;, &quot;-&quot;)
               , &quot;\\&quot;, &quot;-&quot;)
              , &quot;{&quot;, &quot;-&quot;)
             , &quot;|&quot;, &quot;-&quot;)
            , &quot;}&quot;, &quot;-&quot;)
         , &quot;~&quot;, &quot;-&quot;)
        , &quot;\&quot;&quot;, &quot;-&quot;)
       , &quot;#&quot;, &quot;-&quot;)
      , &quot;%&quot;, &quot;-&quot;)
     , &quot;*&quot;, &quot;-&quot;)
    , &quot;:&quot;, &quot;-&quot;)
   , &quot;&lt;&quot;, &quot;-&quot;)
  , &quot;&gt;&quot;, &quot;-&quot;)
 , &quot;?&quot;, &quot;-&quot;) +  
&quot;_&quot; + 
Opportunity__r.Account.Client_ID__c + 
&quot;%2F&quot; + 
text(year( DATEVALUE(Opportunity__r.CreatedDate ) )) + 
 LPAD(TEXT( MONTH(  DATEVALUE(Opportunity__r.CreatedDate ) ) ), 2, &quot;0&quot;) +
 LPAD(TEXT( DAY(  DATEVALUE(Opportunity__r.CreatedDate ) ) ), 2, &quot;0&quot;) + 
&quot;_&quot; + 
Opportunity__r.Application_ID2__c, 
&apos;Click Here&apos;, 
&apos;_blank&apos; 
)</formula>
