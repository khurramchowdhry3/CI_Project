        <formula>CASE(
MOD(Contract_Signed_Received_Date__c - DATE(1900, 1, 7), 7),
0, (Contract_Signed_Received_Date__c) + 9 + FLOOR((9-1)/5)*2,
1, (Contract_Signed_Received_Date__c) + 9 + FLOOR((9)/5)*2,
2, (Contract_Signed_Received_Date__c) + 9 + FLOOR((9+1)/5)*2,
3, (Contract_Signed_Received_Date__c) + 9 + FLOOR((9+2)/5)*2,
4, (Contract_Signed_Received_Date__c) + 9 + FLOOR((9+3)/5)*2,
5, (Contract_Signed_Received_Date__c) + 9 + CEILING((9)/5)*2,
6, (Contract_Signed_Received_Date__c) - IF(9&gt;0,1,0) + 9+ CEILING((9)/5)*2,
null)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
